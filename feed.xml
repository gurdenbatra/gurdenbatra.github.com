    <title>CircuLaw</title>
    <link href="http://localhost:8080/blog/circulaw/" />
    <updated>2023-01-25T00:00:00Z</updated>
    <id>http://localhost:8080/blog/circulaw/</id>
    <content
      xml:lang=""
      type="html"
